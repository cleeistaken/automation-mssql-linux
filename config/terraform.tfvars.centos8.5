template_library_name = "Content Library MSSQL"
template_ova = "centos8.5_x64.ova"
template_name = "CentOS 8.5 x64"
template_description = "CentOS 8.5 x64 for MSSQL Automation"
template_boot = "efi"

#
# Resource Pool
#
resource_pool = "mssql-linux"

#
# MSSQL Linux VM
#
mssql_linux_vm_prefix = "mssql-linux"
mssql_linux_vm_count = 4
mssql_linux_vm = {
    cpu = 16
    memory_gb = 64
    os_disk_gb = 100
    data_disk_gb = 250
}
